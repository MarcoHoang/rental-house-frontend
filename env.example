# API Configuration
VITE_API_URL=http://localhost:8080
VITE_API_TIMEOUT=10000

# Authentication
VITE_JWT_STORAGE_KEY=rental_house_token
VITE_REFRESH_TOKEN_KEY=rental_house_refresh_token

# App Configuration
VITE_APP_NAME=Rental House
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Modern rental house management platform

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,gif,pdf,doc,docx

# Pagination
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# Maps (if using Google Maps)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Payment (if using Stripe)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Email (if using email service)
VITE_EMAIL_SERVICE_URL=https://your-email-service.com

# Social Login (if implemented)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here

# Monitoring
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
